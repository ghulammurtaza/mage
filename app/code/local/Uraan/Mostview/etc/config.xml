<config>
	<global>
		<modules>
                <Uraan_Mostview>
                        <version>1.0.0</version>
                </Uraan_Mostview>
        </modules>
	<blocks>
            <mostview>
                <rewrite>
		 <mostview>Uraan_Mostview_Block_Mostview</mostview>
		</rewrite>
            </mostview>
	 </blocks>

        </global>
	   <frontend>
                <routers>
                        <mostview>
                                <use>standard</use>
                                <args>
                                      <module>Uraan_Mostview</module>
                                      <frontName>mostview</frontName>
                                </args>
                        </mostview>
                </routers>
		<layout>
			<updates>
				<mostview>
		   		      <file>mostview.xml</file>
				</mostview>
			</updates>
       		</layout>
        </frontend>
</config>
